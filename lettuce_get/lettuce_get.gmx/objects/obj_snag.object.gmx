        <arguments>
          <argument>
            <kind>1</kind>
            <string>switch attack
{
    case 0:
    {
        scr_enemy_move();
        image_speed = 0;
        image_index = 0;
        if x = originx &amp;&amp; y = originy 
        {
            image_xscale = -1;
            x_vel = 0;
            x_dir = -1;
            attack = next;
        }
        else if x &lt; originx x_dir = 1;
        else if x &gt; originx x_dir = -1;
        else if x = originx x_dir = 0;
        break;
    }
    case 1:
    {
        scr_enemy_move();
        image_speed = 0;
        image_index = 2;
        if x &gt; obj_player.x &amp;&amp; !alarm[5]
        {
            alarm[5] = 20;
            x_dir = -1;
            if hp &gt; 5 x_cap = 1;
            else x_cap = 2;
        }
        if x &lt; obj_player.x &amp;&amp; !alarm[5]
        {
            alarm[5] = 20;
            x_dir = 1;
            if hp &gt; 5 x_cap = 1;
            else x_cap = 2;
        }
        if x = obj_player.x x_dir = 0;
        if !alarm[4] &amp;&amp; abs(x-obj_player.x) &lt; 100
        {
            x_cap = 4;
            x_vel += 3*x_dir;
            y_vel = -6;
            attack = 5;
        }
        if !alarm[3] &amp;&amp; next = 2 attack = 0;
        else if !alarm[3] &amp;&amp; next = 3
        {
            attack = 6;
            audio_play_sound(sound_snag_jump,0,0);
            x_vel = 0;
            x_dir = 0;
            y_vel = -4.55;
            y_accel = 0.13;
            alarm[7] = 33;
        }
        break;
    }   
    case 2:
    {
        alarm[0] = 1;
        attack = 4;
        break;
    }
    case 3:
    {
        if alarm[2] == -1
        {
            acorn = instance_create(round(random_range(1710,2065)),80,obj_acorn);            
            alarm[2] = 20;
            if count == 20 &amp;&amp; !instance_exists(obj_shell) &amp;&amp; obj_player.shell_on_player != 1
            {
                shell = instance_create(round(random_range(1800,1900)),80,obj_shell);
                shell.hp = 3;
                shell.x_vel = 0;
            }
            count += 1;
        }
        
        if count == 30 &amp;&amp; instance_exists(obj_acorn) 
        {
            attack = 1;
            alarm[4] = 180;
            next = 2;
            alarm[3] = 600;
        }
        if x &gt; obj_player.x x_dir = -1;
        if x &lt; obj_player.x x_dir = 1;
        if x = obj_player.x x_vel = 0
        
        scr_enemy_move();
        image_speed = 0;
        image_index = 2;
        break;
    }
    case 5:
    {
        scr_enemy_move();
        if position_meeting(x,y+1+sprite_height/2,obj_wall)
        {
            attack = 1;
            alarm[4] = 120;
            image_index = spr_snag;
            image_speed = 0;
            x_cap = 1;
            x_vel = 1*x_dir;
        }
        break;
    }
    case 6:
    {
        sprite_index = spr_snag_jump;
        image_speed = 0;
        if alarm[7] != -1
        {
            alarm[6] = 20;
            scr_enemy_move();
        }
        else if alarm[6] != -1 
        {
            y_vel = 0; 
            y_accel = .65;
        }
        else 
        {
            y_vel = 7;
            scr_enemy_move();
        }
        if position_meeting(x,y+1+sprite_height/2,obj_wall)
        {
            attack = 3;
            audio_play_sound(sound_snag_land,0,0);
            audio_play_sound(sound_leaf,0,0);
            obj_view.shake = 10;
            sprite_index = spr_snag;
            image_speed = 0;
        }
        break;
    }
    default: break;
}    

scr_enemy_collide();
if hp &lt; 1
{
rift = instance_create(x,y-50,obj_end_lvl_1_4);
rift.image_xscale = 1;
}
</string>
